{
  "name": "padel-mate-server-core",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && npm run copy-static-assets",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npm run copy-static-assets && nest start --watch",
    "start:debug": "npm run copy-static-assets && nest start --debug --watch",
    "start:prod": "node dist/main",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "postinstall": "patch-package",
    "typeorm:migrate": "yarn build && ./node_modules/typeorm/cli.js migration:generate ./src/migrations/Migration -d ./dist/ormconfig.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@jeremybarbet/apple-api-types": "^1.3.5",
    "@jeremybarbet/google-api-types": "^1.3.5",
    "@jeremybarbet/nest-iap": "^1.3.6",
    "@jeremybarbet/node-iap": "^1.3.6",
    "@mollie/api-client": "^3.7.0-beta.2",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.2.1",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/swagger": "^6.0.5",
    "@nestjs/throttler": "^3.1.0",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.2.1",
    "@socket.io/redis-adapter": "^8.0.1",
    "arraybuffer-to-buffer": "^0.0.7",
    "aws-sdk": "^2.1190.0",
    "axios": "^1.4.0",
    "bull": "^4.10.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-fetch": "^3.1.6",
    "currency-list": "^1.0.8",
    "file-type": "^16.5.4",
    "firebase-admin": "^11.0.1",
    "handlebars": "^4.7.7",
    "libphonenumber-js": "^1.10.18",
    "livekit-server-sdk": "^1.2.1",
    "magic-bytes.js": "^1.0.12",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "node-vibrant": "^3.2.1-alpha.1",
    "passport": "^0.6.0",
    "passport-firebase-jwt": "^1.2.1",
    "pg": "^8.7.3",
    "puppeteer": "^20.5.0",
    "redis": "^4.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sharp": "^0.32.1",
    "shelljs": "^0.8.4",
    "simple-oauth2": "^5.0.0",
    "socket.io": "^4.5.4",
    "sumup-rest-api": "^1.0.1",
    "typeorm": "^0.3.16",
    "typeorm-transactional": "^0.4.1",
    "uuid": "^9.0.0",
    "voucher-code-generator": "^1.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/simple-oauth2": "^4.1.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.2",
    "patch-package": "^6.5.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
